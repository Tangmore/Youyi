/**index.wxss**/
/**功能一:轮播图**/
.slider,.slider swiper,.slider image{
  height:420rpx;
  width:100%;
}
/**功能二:九宫格**/
.index-nav{
   display: flex;
   flex-wrap: wrap;
}
.nav-item{
   width:33.33333%;
   height:140rpx;
   box-sizing: border-box;
   display: flex;
   flex-direction: column;
   justify-content: center;
   align-items: center;
   font-size:14px;
}
.nav-item image{
   width:102rpx;
   height:102rpx;
}
/**功能三:分隔块**/
.divider{
  height:30rpx;
  background-color:#f1f1f1; 
}


